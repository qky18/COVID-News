<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.java.qiukeyue">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_nine"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NewsSearchedActivity"></activity>
        <activity android:name=".NewsViewActivity"></activity>
        <activity android:name=".CategoryActivity"></activity>
        <activity android:name=".ScholarViewActivity"></activity>
        <activity android:name=".EntitySearchedActivity"></activity>
        <activity android:name=".EntityViewActivity"></activity>
        <activity android:name=".NewsOfflineActivity"></activity>
        <activity android:name=".NewsClusterActivity"></activity>
        <meta-data
            android:name="DATABASE"
            android:value="news.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.java.qiukeyue.bean" />
    </application>

</manifest>